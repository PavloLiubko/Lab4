#include <iostream>
#include <numeric>

int gcd(int a, int b) {
    while (b != 0) {
        int c = a % b;
        a = b;
        b = c;
    }
    return a;
}

int main()
{
    setlocale(LC_ALL, "RU"); // українського кодування на жаль не знайшов 
    
    int A, B;
    std::cout << "Введіть значення для числа А:";
    std::cin >> A;
    std::cout << "Введіть значення для числа B:";
    std::cin >> B;

    std::cout << "НСД цих двох чисел = " << gcd(A, B);

    return 0;
}
