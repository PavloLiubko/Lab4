#include <iostream>
#include <ctime>

int main()
{
    setlocale(LC_ALL, "RU"); // українського кодування на жаль не знайшов 
    
    int nums[10];
    srand(time(NULL));

    for (int i = 0; i < 10; i++) {
        nums[i] = 1 + rand() % 50;
    }

    std::cout << "Наші 10 рандомних чисел: \n";

    for (int i = 0; i < 10; i++) {
        std::cout << nums[i] << " \n ";

    }
    int maximum = nums[0];
    for (int el = 0; el < 10; el++) {
        if (nums[el] > maximum) {
            maximum = nums[el];
        }
    }

    std::cout << "Наше найбільше число = " << maximum; 

    return 0;
}
